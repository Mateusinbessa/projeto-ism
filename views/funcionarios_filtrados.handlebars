<h1>Funcionários</h1>
<div id="Funcionarios">
    <table class="tabela">
        <tr>
            <th>Unidade</th>
            <th>Matrícula</th>
            <th>Nome</th>
        </tr>

        {{#each users}}
        <tr>
            <td class="upercase">{{this.unidade}}</td>
            <td>{{this.id}}</td>
            <td>{{this.nome}}</td>
        </tr>
        {{/each}}
    </table>
</div>

